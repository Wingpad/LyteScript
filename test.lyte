Person <- %{
    __constructor: @(name, hats) {
        name -> @name
        hats -> @hats
    },
    numHats: { @hats.length },
    introduce: { Echo("I'm " `++` @name `++` " and I have " `++` @numHats `++` " hats.") },
    printHats: {
        `Echo` "My hat collection includes:"
        For(0, @numHats, @(i) {
            `Echo` @hats[i]
        })
    },
    dance: @(danceName) {
        If (danceName Undefined?, {
            `Echo` "I'm doing my own thing!"
        }, {
            Echo("I'm doing the " `++` danceName)
        })
    }
}

%{"astronaut helmet", "fedora", "feathered hat", "baseball hat"} -> billsHats
Instantiate(Person, "Bill", billsHats) -> bill

bill.introduce()
bill.printHats()
bill.dance("tango")
